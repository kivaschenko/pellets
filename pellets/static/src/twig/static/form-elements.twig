<!doctype html>
<html lang="en">

<head>
  {% include '../include/_meta.twig' %}
  {% include '../include/_font-icon.twig' %}
  <!-- Plugins -->
  <link rel="stylesheet" href="{{ folder.plugins }}duallistbox/duallistbox.min.css">
  <link rel="stylesheet" href="{{ folder.plugins }}star-rating/star-rating.min.css">
  {% include '../include/_main-css.twig' %}
  <title>{{ title }}</title>
</head>

<body>

  {% include '../include/_sidebar.twig' with {
    'menu': 'forms',
    'sub': 'form-elements'
  } %}

  <!-- Main -->
  <div class="main">

    {% include '../include/_header.twig' %}

    <!-- Main body -->
    <div class="main-body">

      <!-- Breadcrumb -->
      <nav aria-label="breadcrumb" class="main-breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="index.html">Home</a></li>
          <li class="breadcrumb-item"><a href="javascript:void(0)">Forms</a></li>
          <li class="breadcrumb-item active" aria-current="page">Elements</li>
        </ol>
      </nav>
      <!-- /Breadcrumb -->

      {% include '../content/_form-elements.twig' %}

    </div>
    <!-- /Main body -->

  </div>
  <!-- /Main -->

  {% include '../include/_modal-search.twig' %}

  {% include '../include/_main-js.twig' %}

  <!-- Plugins -->
  <script src="{{ folder.plugins }}autosize/autosize.min.js"></script>
  <script src="{{ folder.dist }}js/autocomplete.min.js"></script>
  <script src="{{ folder.plugins }}duallistbox/duallistbox.min.js"></script>
  <script src="{{ folder.plugins }}star-rating/star-rating.min.js"></script>
  <script src="{{ folder.plugins }}slugify/slugify.min.js"></script>
  <script>
    {% include '../content/_form-elements.js' %}
  </script>

</body>

</html>
