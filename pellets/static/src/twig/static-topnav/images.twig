<!doctype html>
<html lang="en">

<head>
  {% include '../include/_meta.twig' %}
  {% include '../include/_font-icon.twig' %}
  <!-- Plugins -->
  <link rel="stylesheet" href="{{ folder.plugins }}photoswipe/photoswipe.min.css">
  <link rel="stylesheet" href="{{ folder.plugins }}photoswipe/photoswipe-default-skin/default-skin.min.css">
  <link rel="stylesheet" href="{{ folder.plugins }}imagecompare/imagecompare.min.css">
  {% include '../include/_main-css.twig' %}
  <title>{{ title }}</title>
</head>

<body class="sticky-navbar">

  {% include '../include/_sidebar.twig' with {
    'menu': 'images',
    'sub': ''
  } %}

  <!-- Main -->
  <div class="main">

    {% include '_header.twig' %}

    {% include '_navbar.twig' with {
      'menu': 'images',
      'sub': ''
    } %}

    <!-- Main body -->
    <div class="main-body">

      <!-- Breadcrumb -->
      <nav aria-label="breadcrumb" class="main-breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="index.html">Home</a></li>
          <li class="breadcrumb-item active" aria-current="page">Images</li>
        </ol>
      </nav>
      <!-- /Breadcrumb -->

      {% include '../content/_images.twig' %}

    </div>
    <!-- /Main body -->

  </div>
  <!-- /Main -->

  {% include '../include/_modal-search.twig' %}

  {% include '../include/_main-js.twig' %}

  <!-- Plugins -->
  <script src="{{ folder.plugins }}photoswipe/photoswipe.min.js"></script>
  <script src="{{ folder.plugins }}photoswipe/photoswipe-ui-default.min.js"></script>
  <script src="{{ folder.plugins }}imagecompare/imagecompare.min.js"></script>
  <script>
    {% include '../content/_images.js' %}
  </script>

</body>

</html>
