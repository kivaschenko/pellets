<!-- Sidebar -->
  <div class="sidebar">

    <!-- Sidebar header -->
    <div class="sidebar-header">
      {% include '../content/_sidebar-header.twig' %}
    </div>
    <!-- /Sidebar header -->

    <!-- Sidebar body -->
    <div class="sidebar-body">
      <ul class="nav nav-sub" id="menu">{% for item in menus %}{% if (item.label) %}
        <li class="nav-label">{{ item.label }}</li>{% endif %}{% if (item.subs) %}
        <li class="nav-item">
          <a class="nav-link has-icon dropdown-toggle" href="#">{{ item.icon }}{{ item.title }}{{ item.badge }}</a>
          <ul>{% for subitem in item.subs %}
            <li><a href="{%if(subitem.static)%}../static/{{ subitem.href }}{%else%}#{{ subitem.href|replace({'index': 'dashboard'}) }}{%endif%}.html"{%if(subitem.breadcrumb==false)%} data-breadcrumb="false"{%endif%}>{{ subitem.title }}</a></li>{% endfor %}
          </ul>
        </li>{% else %}
        <li class="nav-item">
          <a class="nav-link has-icon" href="{%if(item.static)%}../static/{{ item.href }}{%else%}#{{ item.href|replace({'index': 'dashboard'}) }}{%endif%}.html"{%if(item.breadcrumb==false)%} data-breadcrumb="false"{%endif%}>{{ item.icon }}{{ item.title }}{{ item.badge }}</a>
        </li>{% endif %}{% endfor %}
      </ul>
    </div>
    <!-- /Sidebar body -->

  </div>
  <!-- /Sidebar -->
